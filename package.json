{
  "name": "id62",
  "version": "2.0.0",
  "description": "Base-62 Random UUIDs",
  "keywords": [
    "base62",
    "id",
    "random",
    "uuid",
    "identifier",
    "guid",
    "unique-id"
  ],
  "homepage": "https://github.com/trevorr/id62#readme",
  "bugs": {
    "url": "https://github.com/trevorr/id62/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorr/id62.git"
  },
  "license": "ISC",
  "author": "Trevor Robinson",
  "funding": "https://github.com/sponsors/trevorr",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "commonjs",
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project src",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:ci": "mkdirp test-results/eslint && eslint --format junit --output-file test-results/eslint/eslint.xml 'src/**/*.ts' 'test/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "pretest": "npm run build",
    "prettier": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "node --import=tsimp/import --test 'test/**/*.test.ts'",
    "test:ci": "npm run build && mkdirp test-results/junit && c8 --100 --reporter=lcov --reporter=html --reporter=text node --import=tsimp/import --test --test-reporter=spec --test-reporter=junit --test-reporter-destination=stdout --test-reporter-destination=test-results/junit/junit.xml 'test/**/*.test.ts'",
    "test:coverage": "npm run build && c8 --100 --reporter=lcov --reporter=html --reporter=text node --import=tsimp/import --test 'test/**/*.test.ts'"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "c8": "^10.1.3",
    "eslint": "^9.39.1",
    "eslint-formatter-junit": "^9.0.1",
    "mkdirp": "^3.0.1",
    "prettier": "^3.6.2",
    "tsimp": "^2.0.12",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
